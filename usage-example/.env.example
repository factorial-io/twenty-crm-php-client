# Twenty CRM API Configuration
# Copy this file to .env and fill in your credentials

# Your Twenty CRM instance URL (with /rest/ endpoint)
TWENTY_API_BASE_URI=https://factorial.twenty.com/rest/

# Your Twenty CRM API token
# Get this from: Settings > Developers > API Keys
TWENTY_API_TOKEN=your_api_token_here

# Test mode (for running integration tests)
TWENTY_TEST_MODE=integration
